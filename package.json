    "next": "^15.5.2",
    "pdf-lib": "^1.17.1",
    "postcss": "^8.4.41",
    "prisma": "^6.16.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.2.0",
    "sharp": "^0.33.4",
    "tailwindcss": "^3.4.9",
    "tsx": "^4.16.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.5.3",
